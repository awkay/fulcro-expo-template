{
  "expo": {
    "name": "Fulcro Expo Template",
    "slug": "fulcro",
    "privacy": "public",
    "entryPoint": "./app/index.js",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "scheme": "fulcro",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": ""
  }
}